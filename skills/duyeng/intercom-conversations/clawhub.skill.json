{
  "name": "intercom_conversations_read",
  "version": "1.0.1",
  "description": "Read Intercom conversations via intercom-client (list/find/search).",
  "entrypoint": "index.js",
  "invoke": "default(input)",
  "runtime": {
    "language": "nodejs",
    "node": ">=18",
    "module_type": "esm"
  },
  "required_env": [
    {
      "name": "INTERCOM_ACCESS_TOKEN",
      "required": true,
      "description": "Intercom Personal Access Token / OAuth access token."
    }
  ],
  "actions": [
    {
      "name": "conversations.list",
      "type": "read"
    },
    {
      "name": "conversations.find",
      "type": "read"
    },
    {
      "name": "conversations.search",
      "type": "read"
    }
  ],
  "docs": {
    "openapi": "openapi.yaml",
    "readme": "README.md"
  }
}