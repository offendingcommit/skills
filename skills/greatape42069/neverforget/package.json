{
  "name": "@degendoge/neverforget",
  "version": "1.0.4",
  "description": "Sovereign Local Vector Memory for Degen Doge. Configures Gemma-300M local embeddings with full environment indexing.",
  "main": "index.js",
  "openclaw": {
    "type": "skill",
    "category": "intelligence",
    "author": "Degen Doge",
    "license": "MIT",
    "permissions": {
      "filesystem": [
        "~/",
        "~/openclaw",
        "~/.openclaw",
        "~/.openclaw/workspace",
        "~/.openclaw/openclaw.json",
        "~/.openclaw/skills/neverforget"
      ],
      "exclude": [
        "**/.openclaw/memory/**",
        "**/node_modules/**",
        "**/.ssh/**",
        "**/.aws/**",
        "**/.gnupg/**",
        "**/.env",
        "**/config/google-chrome/**",
        "**/.mozilla/**"
      ],
      "config": [
        "agents.defaults.memorySearch.provider",
        "agents.defaults.memorySearch.local.modelPath"
      ],
      "network": [
        "pnpm.io",
        "registry.npmjs.org",
        "huggingface.co",
        "hf-mirror.com"
      ]
    },
    "privacy": {
      "dataResidency": "local",
      "modelInference": "local",
      "externalCalls": "Initial setup only (model/package download)"
    }
  }
}