{
  "remote": "tiklick-drive",
  "basePath": "OpenClaw-Backups",
  "retentionDays": 20,
  "sources": [
    "${OPENCLAW_HOME:-$HOME/.openclaw}",
    "${WORKSPACE_DIR:-/workspace}"
  ],
  "excludePatterns": [
    "**/node_modules/",
    "**/.git/",
    "**/dist/",
    "**/build/",
    "**/vendor/",
    "*.log",
    "*.tmp",
    "*.cache"
  ],
  "schedule": "0 3 * * *",
  "alertOnFailure": true,
  "enableEncryption": false,
  "compressionLevel": 6
}